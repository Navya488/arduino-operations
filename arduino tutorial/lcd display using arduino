#include <LiquidCrystal.h>
LiquidCrystal lcd(2,3,4,5,6,7);


void setup() {
  lcd.begin(16,2);
  lcd.setCursor(5,0);
  lcd.print("WELCOME");
  lcd.setCursor(0,1);
  lcd.print("to");
  delay(3000);
  lcd.clear();
  lcd.setCursor(4,0);
  lcd.print("navya");
  lcd.setCursor(5,1);
  lcd.print("ARDUINO UNO");
  

}

void loop() {
  // put your main code here, to run repeatedly:

}
